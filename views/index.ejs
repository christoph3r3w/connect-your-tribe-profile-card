            <img src="" alt="">
            <p></p>
          </li>
          <li>
            <img src="" alt="">
            <p></p>
          </li>
        </menu>
      </div>

    </div>

   

  </section>
 
  <footer class="taskbar">
    <nav>
      <button class="start" popovertarget="start_menu" popovertargetaction="show"><img src="images/pngwing.com.png" alt="windows logo" sizes="20" > <p>start</p></button>
      <div class="bar"></div>
      <div class="extra_icons">
        <ul>
          <li><button class="window_theme"><img src="images/icon/1005.ico" alt="theme icon"></button></li>
          <li><a href="#"><img src="images/icon/975.ico" alt=""> </a></li>
          <li><a href="#"><img src="images/icon/952.ico" alt=""> </a></li>
          <li><p class="current_T">11:11</p></li>
