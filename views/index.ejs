<%- include('./partials/head') %>

<main class="screen">

  <section class="desktop">
    <ul class="desktop_items">

      <li><button href="#"> <img src="images/icon/839.ico" alt=""><div><p>home</p></div></button></li>
      <li><button popovertarget="personal" popovertargetaction="show" > <img src="images/chris icon lowlowres.png" alt=""><div><p>personal</p></div></button></li>
      <li><button popovertarget="music" > <img src="images/icon/1221.ico" alt=""><div><p>Music</p></div></button></li>
      <li><button popovertarget="sprint" popovertargetaction="show" > <img src="images/icon/1224.ico" alt=""><div><p>sprint</p></div></button></li>
      <li><button><a href="<%= website %>"><img src="images/github.png" sizes="80" alt=""><div><p>github</p></div></a></button></li>
      
    </ul>
    
    <div class="trash_can"><a href="#"> <img src="images/icon/261.ico" alt=""></a></div>

         
      <div popover id="personal" class="window_card" >
        <%- include('./partials/window_personal', {window_personal: ''}) %> 
      </div>

      <div popover id="sprint" class="window_card" >
        <%- include('./partials/window_sprint', {window_sprint: ''}) %> 
      </div>

      <div popover="auto" id="music" class="window_card" >
